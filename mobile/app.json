{
  "expo": {
    "name": "nutrition-care-mobile",
    "slug": "nutrition-care-mobile",
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": { "projectId": "local" }
    },
    "experiments": {
      "typedRoutes": false
    }
  },
  "expoClient": {
    "extra": {
      "EXPO_PUBLIC_API_BASE": "http://localhost:8000/api",
      "EXPO_PUBLIC_SOCKET_URL": "http://localhost:4000"
    }
  }
}

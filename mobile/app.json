{
  "expo": {
    "name": "Nutrition Care",
    "slug": "nutrition-care-mobile",
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F7FA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.nutrition-care-mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.anonymous.nutrition_care_mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "local"
      },
      "EXPO_PUBLIC_ONESIGNAL_APP_ID": "c5e4f3ad-84d0-40d0-ae1c-a366bb662f3b"
    },
    "experiments": {
      "typedRoutes": false
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ]
  },
  "expoClient": {
    "extra": {
      "EXPO_PUBLIC_API_BASE": "http://localhost:8000/api",
      "EXPO_PUBLIC_SOCKET_URL": "http://localhost:4000"
    }
  }
}